            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\n  project as '项目',\n  COUNT(*) as '事件数',\n  COUNT(DISTINCT event) as '事件类型数',\n  COUNT(DISTINCT user_agent) as '用户代理数'\nFROM video_metrics\nWHERE event_time >= FROM_UNIXTIME($__from/1000) AND event_time <= FROM_UNIXTIME($__to/1000)\nGROUP BY project\nORDER BY COUNT(*) DESC",
            "refId": "A"
          }

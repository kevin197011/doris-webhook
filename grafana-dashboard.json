            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\n  event as '事件类型',\n  COUNT(*) as '事件数',\n  COUNT(DISTINCT project) as '项目数',\n  ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM video_metrics WHERE event_time >= FROM_UNIXTIME($__from/1000) AND event_time <= FROM_UNIXTIME($__to/1000)), 2) as '占比(%)'\nFROM video_metrics\nWHERE event_time >= FROM_UNIXTIME($__from/1000) AND event_time <= FROM_UNIXTIME($__to/1000)\nGROUP BY event\nORDER BY COUNT(*) DESC",
            "refId": "A"
          }

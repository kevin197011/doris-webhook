            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\n  project as '项目',\n  event as '事件类型',\n  COUNT(*) as '事件数',\n  ROUND(COUNT(*) * 100.0 / SUM(COUNT(*)) OVER (PARTITION BY project), 2) as '项目内占比(%)',\n  ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM video_metrics), 2) as '总占比(%)'\nFROM video_metrics\nGROUP BY project, event\nORDER BY project, COUNT(*) DESC",
